<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title – приложение ASP.NET</title>
    @Styles.Render("~/Content/style.css")
</head>
<body style="padding-top:0px;">
    <div class="background"></div>
    <nav class="navbar navbar-expand-lg navbar-ligth bg-info mr-lg-10">
        <div class="collapse navbar-collapse" id="navbarcontent">
            <div class="navbar-nav mr-auto">
                <button type="button" class="btn btn-info mr-1" href="#">Home</button>
                <button type="button" class="btn btn-info mr-1" href="#">Log in</button>
                <button type="button" class="btn btn-info mr-1" href="#">Sign up</button>
                <button type="button" class="btn btn-info mr-1" href="#">Google</button>
                <button type="button" class="btn btn-info mr-50" href="#">YouTube</button>

                <form class="form-inline ml-xl-5">
                    <input class="form-control mr-sm-2" type="search" placeholder="Search" aria-label="Search">
                    <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
                </form>
            </div>

        </div>
    </nav>

    @RenderBody()

</body>
</html>
