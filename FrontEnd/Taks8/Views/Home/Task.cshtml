@model string[]
@{
    Layout = null;
}


@Styles.Render("~/Content/Site.css")
@Scripts.Render("~/Scripts/jquery-3.3.1.js")
@Styles.Render("~/Content/bootstrap.min.css")

<div class="container" style="display: flex; justify-content: space-around;">
    <div class="col-4" style="display: flex; flex-direction: column;">
        <label>Countries</label>
        <select name="leftSelect" multiple>
            @foreach (var item in Model)
            {
                <option>@item</option>
            }
        </select>
    </div>
    <div class="col-4">
        <button name="moveright" class="button">></button>
        <button name="moveleft" class="button"> < </button>
        <button name="moveallright" class="button"> >> </button>
        <button name="moveallleft" class="button"> << </button>
    </div>
    <div class="col-4" style="display: flex; flex-direction: column; justify-content: center;">
        <label>Selected countries</label>
        <select name="rigthSelect" multiple>

        </select>
    </div>
</div>

<script>
    $("")
</script>



@Scripts.Render("~/Scripts/Site.js")
